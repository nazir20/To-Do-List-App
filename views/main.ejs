<%- include('header') -%>
    
    <div id="heading" class="box">
        <h1><%= toDoListTitle %> </h1>
    </div>
    <!--  -->
    <div class="box">
        <% for(let i = 0; i<newListItems.length; i++){ %>
            <div class="item">
                <input type="checkbox">
                <p><%=newListItems[i]%></p>
            </div>
        <%} %>
        <form class="item" action="/" method="post">
            <input autocomplete="off" type="text" name="newItem" placeholder="New Item">
            <button type="submit" name="list" value=<%= toDoListTitle %>>+</button>
        </form>
    </div>











<%- include('footer') -%>